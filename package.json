{
  "name": "triage",
  "version": "0.0.0",
  "description": "Router scaffolding for connect/express apps",
  "main": "index.js",
  "scripts": {
    "test": "npm run-script lint && npm run-script coverage && npm run-script shippable-report",
    "lint": "./node_modules/.bin/jshint -c .jshintrc lib/ test/",
    "coverage": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- --recursive test",
    "shippable-report": "./node_modules/.bin/istanbul report cobertura --dir  shippable/codecoverage/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shane-tomlinson/triage.git"
  },
  "keywords": [
    "connect",
    "express",
    "router",
    "route"
  ],
  "author": "Shane Tomlinson",
  "license": "MPL2.0",
  "dependencies": {
    "bluebird": "1.2.4",
    "cors": "2.3.1",
    "express": "4.4.0",
    "glob-require": "0.0.1",
    "joi": "4.5.0"
  },
  "devDependencies": {
    "chai": "1.9.1",
    "istanbul": "0.2.10",
    "jshint": "2.5.1",
    "mocha": "1.20.0"
  }
}
